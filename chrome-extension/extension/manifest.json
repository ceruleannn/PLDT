{
  "name": "priceless data",
  "description" : "backup all history data",
  "version": "1.0",
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "img/hello_extensions.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "homepage_url": "http://www.hyhello.com",
  "permissions":
  [
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "bookmarks",
	"contextMenus",
    "<all_urls>"
  ]
}
