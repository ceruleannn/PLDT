<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.hyhello.priceless.dataaccess.entity.Note">
        <table name="tb_note" schema="hyhello" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="noteId">
                <column name="noteId" precision="11"/>
            </basic>
            <basic name="uid">
                <column name="uid" precision="11"/>
            </basic>
            <basic name="text">
                <column name="text"/>
            </basic>
            <basic name="createTime">
                <column name="createTime" precision="-1"/>
            </basic>
            <basic name="version">
                <column name="version" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
